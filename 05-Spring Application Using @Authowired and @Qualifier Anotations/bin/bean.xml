<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">


<context:annotation-config/>
<bean id="octHeart" class="com.ismaeel.autowired.Heart">

	<property name="animalType" value="Oct" />
	<property name="noOfHearts" value="3" />

</bean>

<bean id="humanHeart" class="com.ismaeel.autowired.Heart">

	<property name="animalType" value="Human" />
	<property name="noOfHearts" value="1" />

</bean>

<bean id="body" class="com.ismaeel.autowired.Body"/>

</beans>
